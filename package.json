{
  "name": "hs-react-template",
  "version": "0.1.0",
  "description": "Template for react based services",
  "main": "dist/widgetUI.js",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=22.2.0",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "test": "jest",
    "start-server": "npm run build && npx live-server . --no-browser",
    "dev": "tsc-watch --noClear -p tsconfig.json --onSuccess \"npm run gulp-tasks\"",
    "build:processor": "tsc -p tsconfig-processor.json && npm run copyFiles",
    "build": "rimraf dist/ && webpack --mode production && npm run build:processor",
    "prepare": "npm run build",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.mjs src/**/*.ts",
    "copyFiles": "copyfiles -f src/manifest.json src/icon.svg dist/",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "bump-patch": "npm version patch",
    "bump-minor": "npm version minor",
    "bump-major": "npm version major"
  },
  "author": "Alexander Agudelo",
  "license": "UNLICENSED",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@kemu-io/hs": "https://github.com/kemu-io/hs#latest",
    "@kemu-io/hs-react": "https://github.com/kemu-io/hs-react#latest",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@eslint/js": "^9.4.0",
    "@types/babel__core": "^7",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.21.2",
    "eslint": "^8.57.0",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.4",
    "gulp-strip-comments": "^2.6.0",
    "gulp-terser": "^2.1.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "live-server": "^1.2.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
