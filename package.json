{
  "name": "hs-react-template",
  "version": "0.1.0",
  "description": "Template for react based services",
  "main": "dist/widgetUI.js",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=22.2.0",
    "npm": ">=10.7.0"
  },
  "type": "module",
  "scripts": {
    "test": "jest",
    "release": "gulp release",
    "start-server": "npm run copyFiles && npm run copyFolders && webpack serve",
    "dev": "tsc-watch --noClear -p tsconfig.json --onSuccess \"npm run build\"",
    "build:processor": "tsc -p tsconfig-processor.json && npm run copyFiles && npm run copyFolders",
    "build": "rimraf dist/ && webpack --mode production && npm run build:processor",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint -c eslint.config.mjs src/**/*.ts",
    "copyFolders": "copyfiles -u 1 src/assets/**/* src/customWidgets/*.json dist/",
    "copyFiles": "copyfiles -f src/manifest.json src/*.svg src/assets/**/* dist/"
  },
  "author": "Alexander Agudelo",
  "license": "UNLICENSED",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@kemu-io/hs": "^0.4.48",
    "@kemu-io/hs-react": "^0.2.37",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@emotion/babel-preset-css-prop": "^11.11.0",
    "@eslint/js": "^9.4.0",
    "@types/babel__core": "^7",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.16",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "del": "^7.1.0",
    "esbuild": "^0.21.2",
    "eslint": "^8.57.0",
    "gulp": "^4.0.2",
    "gulp-replace": "^1.1.4",
    "gulp-strip-comments": "^2.6.0",
    "gulp-terser": "^2.1.0",
    "gulp-zip": "^6.0.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
